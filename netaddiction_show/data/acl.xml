<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
        <record model="ir.module.category" id="netaddiction_show_group">
            <field name="name">Netaddiction Fiere</field>
            <field name="description">Quelli che fanno le fiere</field>
            <field name="sequence">99</field>
        </record>

        <record id="netaddiction_show_manager" model="res.groups">
            <field name="name">Utente Manager</field>
            <field name="category_id" ref="netaddiction_show_group"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="menu_netaddiction_show" model="ir.ui.menu">
            <field name="groups_id" eval="[(4, ref('netaddiction_show_manager'))]"/>
        </record>
        <record id="menu_netaddiction_show_base" model="ir.ui.menu">
            <field name="groups_id" eval="[(4, ref('netaddiction_show_manager'))]"/>
        </record>

        <record id="netaddiction_show_access" model="ir.model.access">
            <field name="name">Netaddiction Show</field>
            <field name="model_id" search="[('model','=','netaddiction.show')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_show_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="netaddiction_show_quant_access" model="ir.model.access">
            <field name="name">Netaddiction Show Quant</field>
            <field name="model_id" search="[('model','=','netaddiction.show.quant')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_show_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="netaddiction_return_quant_access" model="ir.model.access">
            <field name="name">Netaddiction Return Quant</field>
            <field name="model_id" search="[('model','=','netaddiction.return.quants')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_show_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="netaddiction_sell_quant_access" model="ir.model.access">
            <field name="name">Netaddiction Sell Quant</field>
            <field name="model_id" search="[('model','=','netaddiction.sell.quant')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_show_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="netaddiction_show_returned_access" model="ir.model.access">
            <field name="name">Netaddiction Show Return Move</field>
            <field name="model_id" search="[('model','=','netaddiction.show.returned.move')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_show_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="netaddiction_difference_quant_access" model="ir.model.access">
            <field name="name">Netaddiction Difference Move</field>
            <field name="model_id" search="[('model','=','netaddiction.difference.quant')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_show_manager"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
    </data>
</openerp>