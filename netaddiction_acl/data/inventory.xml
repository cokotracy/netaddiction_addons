        </record>


        <!--Modelli-->
        <!--Utente-->
        <record id="netaddiction_inventory_user_acces_stock_picking_wave" model="ir.model.access">
            <field name="name">Stock Picking Wave</field>
            <field name="model_id" search="[('model','=','stock.picking.wave')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_inventory_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="0"/>
            <field name="perm_write" eval="0"/>
            <field name="perm_unlink" eval="0"/>
        </record>

        <!--Utente Plus-->
        <record id="netaddiction_inventory_user_plus_acces_stock_picking_wave" model="ir.model.access">
            <field name="name">Stock Picking Wave</field>
            <field name="model_id" search="[('model','=','stock.picking.wave')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_inventory_user_plus"/>
            <field name="perm_read" eval="1"/>
