            <field name="perm_unlink" eval="0"/>
        </record>
        <record id="netaddiction_inventory_user_plus_acces_manifest" model="ir.model.access">
            <field name="name">Manifest</field>
            <field name="model_id" search="[('model','=','netaddiction.manifest')]" model="ir.model"/>
            <field name="group_id" ref="netaddiction_inventory_user_plus"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval=""/>
            <field name="perm_write" eval=""/>
            <field name="perm_unlink" eval="0"/>
        </record>
    </data>
