        </record>
        <record id="stock.menu_report_stock_line_date" model="ir.ui.menu">
            <field name="groups_id" eval="[(4, ref('netaddiction_inventory_user_manager'))]"/>
        </record>
        <record id="stock.menu_stock_procurement_action" model="ir.ui.menu">
