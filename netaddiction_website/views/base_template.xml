<odoo>
    <template id="assets_frontend" inherit_id="web.assets_frontend" name="">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/netaddiction_website/static/src/scss/main.scss"/>
        </xpath>
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/netaddiction_website/static/src/js/main_page.js"></script>
        </xpath>
    </template>

    <template id="main_template" inherit_id="website.layout">
        <xpath expr="//div[@id='wrapwrap']/header/nav" position="before">
           <t t-call="netaddiction_website.header_template"/>
        </xpath>
        <xpath expr="//div[@id='wrapwrap']/header/nav/div/a" position="attributes">
            <attribute name="class" separator=" " add="d-none"/>
        </xpath>
        <xpath expr="//div[@id='wrapwrap']/header/nav/div/div[last()]/ul/li" position="before">
            <li>
                <div class="dropdown">
                    <button class="btn  dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Choose By<strong> Category</strong>
                    </button>

                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                        <a class="dropdown-item" href="#">Action</a>
                        <a class="dropdown-item" href="#">Another action</a>
                        <a class="dropdown-item" href="#">Something else here</a>
                    </div>
                </div>
            </li>
        </xpath>
        <xpath expr="//div[@id='wrapwrap']/header/nav/div/div/ul" position="attributes">
            <attribute name="class" separator=" " remove="ml-auto"/>
        </xpath>
        <xpath expr="//footer" position="replace">
            <t t-call="netaddiction_website.footer_template"/>
        </xpath>
    </template>
    <template id="home_page" inherit_id="website.homepage">
        <xpath expr="//div[@id='wrap']" position="replace">
            <div class="main_page" groups="" id="main_page">
                <div class="col-12 col-lg-11 m-auto">
                    <div class="" id="promo_banner">
                        <a href="https://t.me/multiplayershop" class="text-center">
                            <img style="width:100%;" src="/netaddiction_website/static/src/img/mcombm-telegramshipping-1440x120.jpg" class="img-responsive m15t" data-src="/media/images/banner/2019/11/14/mcombm-telegramshipping-1440x120.jpg" data-src_mobile="/media/images/banner/2019/11/14/mcombm-telegramshipping-1440x120_mBKItkb.jpg"/>
                        </a>
                    </div>
                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img class="d-block w-100" src="/netaddiction_website/static/src/img/Artwork_1440x300.png" alt="First slide"/>
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="/netaddiction_website/static/src/img/FUnko_pop.jpg" alt="Second slide"/>
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="/netaddiction_website/static/src/img/Artwork_1440x300.jpg" alt="Third slide"/>
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="/netaddiction_website/static/src/img/sabbia_2.jpg" alt="Forth slide"/>
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                    <!-- <i class="fa fa-arrow-left"/> -->
                    <div class="">
                        <h2 class="titolo_riga">
                            I Pi√π venduti
                        </h2>
                    </div>
                    <div class="s_products_mostly_sold">
                        <t t-call="website_sale.s_products_recently_viewed">
                           <!--  <xpath expr="." position="inside">
                                Hello this is 
                            </xpath> -->
                        </t>
                    </div>
                    <!-- <i class="float-right fa fa-arrow-right"/> -->
                    <div class="row cover-normale-home m-auto pull-left">
                        <div class="row img-main riga-immagini-home">
                            <div class="img-row col-4 column-img width-1-3 text-center m30b item-same-height-box">
                                <a href="https://multiplayer.com/ricerca/?categoria=Figures%7C%2A&amp;sort=novita&amp;s=">
                                    <img src="/netaddiction_website/static/src/img/Artwork_720x600_1_hKult81.jpg" src-1="/media/images/cover/2018/08/03/Artwork_720x600_1_hKult81.jpg" alt="images / cover / 2018/08/03 / Artwork_720x600_1_hKult81.jpg" class="w-100 img-responsive"/>
                                </a>
                            </div>
                            <div class="img-row col-4 column-img width-1-3 text-center m30b item-same-height-box">
                                <a href="https://multiplayer.com/prodotti/ultime-uscite/?categoria=Modellismo+e+Model+Kit%7C%2A&amp;s=">
                                    <img src="/netaddiction_website/static/src/img/preview-full-models-ultime.jpg" src-1="/media/images/cover/2018/06/04/preview-full-models-ultime.jpg" alt="images / cover / 2018/06/04 / preview-full-models-ultime.jpg" class=" w-100 img-responsive"/>
                                </a>
                            </div>
                            <div class="img-row col-4 column-img width-1-3 text-center m30b item-same-height-box">
                                <a href="https://multiplayer.com/prodotti/ultime-uscite/?categoria=Figures%7C%2A&amp;s=">
                                    <img src="/netaddiction_website/static/src/img/preview-full-figure-ultime.jpg" src-1="/media/images/cover/2018/06/04/preview-full-figure-ultime.jpg" alt="images / cover / 2018/06/04 / preview-full-figure-ultime.jpg" class=" w-100 img-responsive"/>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="row cover-normale-home m-auto pull-left">
                        <div class=" row  img-main riga-immagini-home">
                            <div class="img-row column-img col-4 width-1-3 text-center m30b item-same-height-box">
                                <a href="https://multiplayer.com/ricerca/?s=&amp;attributo=Marca%7CMultiplayer+Edizioni">
                                    <img src="/netaddiction_website/static/src/img/edizioni-260117.jpg" src-1="/media/images/cover/2017/01/27/edizioni-260117.jpg" alt="images / cover / 2017/01/27 / editions-260117.jpg" class="img-responsive w-100"/>
                                </a>
                            </div>
                            <div class="img-row column-img col-4 width-1-3 text-center m30b item-same-height-box">
                                <a href="https://multiplayer.com/prodotti/selezione/multiplayer-things.html">
                                    <img src="/netaddiction_website/static/src/img/robot-260117.jpg" src-1="/media/images/cover/2017/01/27/robot-260117.jpg" alt="images / cover / 2017/01/27 / robot-260117.jpg" class="img-responsive w-100"/>
                                </a>
                            </div>
                            <div class="img-row column-img col-4 width-1-3 text-center m30b item-same-height-box">
                                <a href="https://multiplayer.com/ricerca/?categoria=Giochi%7C%2A&amp;s=&amp;attributo=Marca%7CNintendo">
                                    <img src="/netaddiction_website/static/src/img/plush-260117_1.jpg" src-1="/media/images/cover/2017/01/27/plush-260117_1.jpg" alt="images / cover / 2017/01/27 / plush-260117_1.jpg" class="img-responsive w-100"/>
                                </a>
                            </div>
                                
                        </div>
                    </div>
                </div>
            </div>
        </xpath>
    </template>
</odoo>

    