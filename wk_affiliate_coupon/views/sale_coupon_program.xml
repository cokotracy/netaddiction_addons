<odoo>
    <record id="inherit_sale_coupon_program_view_form" model="ir.ui.view">
      <field name="name">inherit.sale.coupon.program</field>
      <field name="model">sale.coupon.program</field>
      <field name="inherit_id" ref="sale_coupon.sale_coupon_program_view_form_common"/>
      <field name="arch" type="xml">
      <xpath expr="//field[@name='discount_fixed_amount']" position="after">
        <notebook>
        <page string="Visit Details">
          <group>
            <field name="aff_visit_id" required="1" group="sales_team.group_sale_manager"/>
          </group>
        </page>
      </notebook>

      </xpath>
    </field>
    </record>
</odoo>
