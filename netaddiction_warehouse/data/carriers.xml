<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record model="res.partner" id="partner_brt">
            <field name="name">BRT</field>
    	</record>
    	<record model="res.partner" id="partner_sda">
            <field name="name">SDA</field>
    	</record>

    	<record model="delivery.carrier" id="carrier_brt">
            <field name="name">Corriere Espresso BRT</field>
            <field name="delivery_type">fixed</field>
            <field name="time_to_shipping">1</field>
            <field name="fixed_price">4.90</field>
            <field name="free_if_more_then">True</field>
            <field name="amount">99</field>
            <field name="partner_id" eval="ref('partner_brt')"/>
    	</record>
    	<record model="delivery.carrier" id="carrier_sda">
            <field name="name">Corriere Espresso SDA</field>
            <field name="delivery_type">fixed</field>
            <field name="time_to_shipping">1</field>
            <field name="fixed_price">4.90</field>
            <field name="free_if_more_then">True</field>
            <field name="amount">99</field>
            <field name="partner_id" eval="ref('partner_sda')"/>
    	</record>

    	<record model="delivery.carrier" id="carrier_altro">
            <field name="name">Altro</field>
            <field name="delivery_type">fixed</field>
            <field name="time_to_shipping">1</field>
            <field name="fixed_price">0</field>
            <field name="partner_id" eval="ref('base.main_company')"/>
    	</record>
    	<record model="delivery.carrier" id="carrier_magazzino">
            <field name="name">Ritiro c/o Magazzino</field>
            <field name="delivery_type">fixed</field>
            <field name="time_to_shipping">1</field>
            <field name="fixed_price">0</field>
            <field name="partner_id" eval="ref('base.main_company')"/>
    	</record>
    </data>
</openerp>