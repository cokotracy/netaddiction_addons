<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <template id="netaddiction_warehouse_assets_backend" name="netaddiction_warehouse_assets_backend" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/netaddiction_warehouse/static/src/css/backend_style.css" />
                <script type="text/javascript" src="/netaddiction_warehouse/static/src/js/widget.js"/>
                <script type="text/javascript" src="/netaddiction_warehouse/static/src/js/reso_cliente.js"/>
            </xpath>
        </template>


        <record id="action_controllo_pickup_2" model="ir.actions.client">
            <field name="name">Controllo Pick Up</field>
            <field name="tag">netaddiction_warehouse.controllo_pickup</field>
        </record>

        <menuitem id="netaddiction_warehouse_controllo_pickup" name="Controllo Pickup" parent="stock.menu_stock_warehouse_mgmt"
            action="action_controllo_pickup_2"/>

        <record id="action_carico" model="ir.actions.client">
            <field name="name">Carico da Fornitore</field>
            <field name="tag">netaddiction_warehouse.carico</field>
        </record>

        <menuitem id="netaddiction_warehouse_carico" name="Carico da Fornitore" parent="stock.menu_stock_warehouse_mgmt"
            action="action_carico"/>

        <record id="action_reso_cliente" model="ir.actions.client">
            <field name="name">Reso Cliente</field>
            <field name="tag">netaddiction_warehouse.reso_cliente</field>
        </record>
    </data>
</openerp>